Options +FollowSymlinks
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
#RewriteRule ^(.*)\.htm$ $1.php [NC]

#RewriteRule ^/([^/]+)/([^/]+)/? ./index.php?page_slug=$1&posts_id=$2 [QSA]
#RewriteRule ^/([^/]+)/([^/]+)/? ./index.php?page_slug=$1&posts_id=$2 [QSA]
#RewriteRule ^([^/d]+)/?$ ./index.php?page_slug=$1 [QSA]
#RewriteRule ^index/([^/]+)$ /index.php?page_slug=$1 [NC]
#RewriteRule ^id/([^/]+)/$ ./index.php?id=$1
RewriteRule ^([^/]+)/?$ /index.php?page_slug=$1 [NC,L,QSA]
RewriteRule ^bids/([^/]+)/?$ /index.php?posts_id=$1 [NC,L,QSA]
ErrorDocument 404 /404.php